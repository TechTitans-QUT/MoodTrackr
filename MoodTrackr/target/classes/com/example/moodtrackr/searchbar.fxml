<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="searchbar" alignment="CENTER" prefHeight="100" prefWidth="1000" style="-fx-background-color: #252322;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.moodtrackr.controllers.BaseUITemplate">
    <Label contentDisplay="BOTTOM" underline="true"  textFill="WHITE" prefHeight="62.0" prefWidth="268.0" text="MoodTrackr" textAlignment="CENTER">
        <font>
            <Font name="Wingdings" size="24.0" />
        </font>
    </Label>
    <Label contentDisplay="BOTTOM" underline="true"  textFill="WHITE" prefHeight="62.0" prefWidth="110.0" text="Welcome, User" textAlignment="CENTER">
        <font>
            <Font name="Wingdings" size="14.0" />
        </font>
    </Label>
    <TextField prefHeight="20.0" prefWidth="300.0" promptText="Search...">
        <HBox.margin>
            <Insets left="50.0" top="5.0" />
        </HBox.margin>
    </TextField>
    <MenuButton mnemonicParsing="false" text="MenuButton">
        <items>
            <MenuItem mnemonicParsing="false" text="Account" />
            <MenuItem mnemonicParsing="false" text="Notifications" />
            <MenuItem mnemonicParsing="false" text="Logoff" />
        </items>
        <HBox.margin>
            <Insets left="100.0" top="5.0" />
        </HBox.margin>
    </MenuButton>
    <VBox.margin>
        <Insets />
    </VBox.margin>
</HBox>