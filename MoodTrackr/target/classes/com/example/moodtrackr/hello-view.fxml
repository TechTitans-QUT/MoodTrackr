<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox spacing="50.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.moodtrackr.controllers.HelloController">

    <padding>
        <Insets bottom="100.0" left="20.0" right="100.0" top="40.0" />
    </padding>


    <VBox>
        <Label text="Moodtrackr">
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="100.0" />
            </padding></Label>
        <Button text="Dashboard" />
        <Button fx:id="MoodInputButton" onAction="#onMoodInputButtonClick" text="Mood" />
        <Button text="Data" />
        <Button text="Calendar" />
        <Button text="Settings">
            <VBox.margin>
                <Insets bottom="250.0" />
            </VBox.margin></Button>
        <Button mnemonicParsing="false" text="Logout" />
        <HBox.margin>
            <Insets left="20.0" />
        </HBox.margin>
    </VBox>


    <VBox alignment="TOP_CENTER" spacing="20.0" VBox.vgrow="ALWAYS">
        <HBox>
            <Label alignment="TOP_LEFT" text="DASHBOARD">
                <font>
                    <Font size="24.0" />
                </font></Label>
            <TextField prefHeight="5.0" prefWidth="400.0">
                <HBox.margin>
                    <Insets left="50.0" top="5.0" />
                </HBox.margin>
            </TextField>
            <MenuButton mnemonicParsing="false" text="MenuButton">
                <items>
                    <MenuItem mnemonicParsing="false" text="Account" />
                    <MenuItem mnemonicParsing="false" text="Notifications" />
                    <MenuItem mnemonicParsing="false" text="Logoff" />
                </items>
                <HBox.margin>
                    <Insets left="100.0" top="5.0" />
                </HBox.margin>
            </MenuButton>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>

    </VBox>




</HBox>
